<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="EPAM Mobile cloud Web mobile testing Suite">
    <parameter name="appType" value="web" />
    <parameter name="platformName" value="Android" />
    <parameter name="browserName" value="Chrome" />

    <test name="Cloud Web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <classes>
            <class name="hw.scenarios.WebMobileTests" />
        </classes>
    </test>
</suite>
